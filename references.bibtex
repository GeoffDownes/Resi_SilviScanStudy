@article{RN1371,
   author = {von Zedtwitz, Maximilian and Gassmann, Oliver and Boutellier, Roman},
   title = {Organizing global R&D: challenges and dilemmas},
   journal = {Journal of International Management},
   volume = {10},
   number = {1},
   pages = {21-49},
   abstract = {After more than a decade of widespread global R&D expansion, top managers in multinational companies take decentralized competencies for granted, expecting their international research and product development functions to deliver results. However, based on more than 150 in-depth interviews and case research carried out with 18 multinational companies from three industry groups between 1996 and 2000, we have identified six fundamental dilemmas that make it difficult even for companies with carefully managed distributed R&D networks to exploit the full potential of global innovation. In addition to a root-problem analysis, we surveyed these companies about the drivers of R&D globalization, and how these drivers would affect their organizations over the next 10 years. Although some of the trends that emerged are industry specific with regard to technology development, we describe five common traits that are expected to shape R&D organization in the mid-term future.},
   keywords = {Globalization
International innovation
R&amp
D organization
Project management
Knowledge management},
   ISSN = {1075-4253},
   DOI = {http://dx.doi.org/10.1016/j.intman.2003.12.003},
   url = {http://www.sciencedirect.com/science/article/pii/S1075425303000954},
   year = {2004},
   type = {Journal Article}
}

